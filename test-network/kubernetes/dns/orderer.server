orderer.example.com:53 {
    errors
    cache 30

    rewrite stop {
    name regex orderer.example.com orderer1-orderer.test-network.svc.cluster.local
    answer name orderer1-orderer.test-network.svc.cluster.local orderer.example.com
    } 

    kubernetes cluster.local in-addr.arpa ip6.arpa {
    pods insecure
    upstream
    fallthrough in-addr.arpa ip6.arpa
    }
    forward .  /etc/resolv.conf 
}
